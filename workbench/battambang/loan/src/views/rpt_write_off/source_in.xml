           $sub_pri_khr_usd = $con_cur->toUSD(1,$pri_khr,$exchange_rate_id);
           $sub_int_khr_usd= $con_cur->toUSD(1,$int_khr,$exchange_rate_id);
           $sub_pen_khr_usd= $con_cur->toUSD(1,$pen_khr,$exchange_rate_id);
