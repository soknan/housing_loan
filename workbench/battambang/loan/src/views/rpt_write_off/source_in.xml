              $pri_usd=0;
              $int_usd=0;
              $pen_usd=0;
              $total_repay_usd=0;

              $pri_thb=0;
              $int_thb=0;
              $pen_thb=0;
              $total_repay_thb=0;

              $con_cur = new Battambang\Cpanel\Libraries\Currency();
              foreach($result as $key=>$row){
              switch ($row->cp_currency_id){
              case 1:
                  $pri_khr+=$con_cur->toKHR(1,$row->balance_principal,$exchange_rate_id);
                  $int_khr+=$con_cur->toKHR(1,$row->balance_interest,$exchange_rate_id);
                  $pen_khr+=$con_cur->toKHR(1,$row->arrears_penalty,$exchange_rate_id);

              break;
              case 2:
                  $pri_usd+=$con_cur->toUSD(2,$row->balance_principal,$exchange_rate_id);
                  $int_usd+=$con_cur->toUSD(2,$row->balance_interest,$exchange_rate_id);
                  $pen_usd+=$con_cur->toUSD(2,$row->arrears_penalty,$exchange_rate_id);

              break;
              case 3:
                  $pri_thb+=$con_cur->toTHB(3,$row->balance_principal,$exchange_rate_id);
                  $int_thb+=$con_cur->toTHB(3,$row->balance_interest,$exchange_rate_id);
                  $pen_thb+=$con_cur->toTHB(3,$row->arrears_penalty,$exchange_rate_id);

              break;
              }
