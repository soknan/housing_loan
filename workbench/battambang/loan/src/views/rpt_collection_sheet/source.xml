              $sub_total_khr_usd= $con_cur->toUSD(1,$total_col_khr,$exchange_rate_id);

              $sub_pri_thb_usd = $con_cur->toUSD(3,$col_pri_thb,$exchange_rate_id);
              $sub_int_thb_usd= $con_cur->toUSD(3,$col_int_thb,$exchange_rate_id);
              $sub_pen_thb_usd= $con_cur->toUSD(3,$col_pen_thb,$exchange_rate_id);
              $sub_total_thb_usd= $con_cur->toUSD(3,$total_col_thb,$exchange_rate_id);

              $pri_to_USD = $sub_pri_khr_usd + $col_pri_usd + $sub_pri_thb_usd;
              $int_to_USD = $sub_int_khr_usd + $col_int_usd + $sub_int_thb_usd;
              $pen_to_USD = $sub_pen_khr_usd + $col_pen_usd + $sub_pen_thb_usd;
              $total_to_USD = $sub_total_khr_usd + $sub_total_thb_usd + $total_col_usd;

              ?>
